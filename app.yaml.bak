application: private
version: 1
runtime: python27
api_version: 1
threadsafe: yes

handlers:
#  a static file mapping
- url: /
  static_files: login.html
  upload: login.html
  
#  a static file mapping
- url: /index.html
  static_files: index.html
  upload: index.html 
  
# images
- url: /Images
  static_dir: Images
 
 #css style file  
- url: /login.css
  static_files: login.css
  upload: login.css
  
 #js style file  
- url: /javascript
  static_dir: javascript
  
 #css style file  
- url: /index.css
  static_files: index.css
  upload: index.css
   
- url: .*
  script: main.app



